<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" version="2.4">
  <welcome-file-list>
    <welcome-file>sign-in.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>config</servlet-name>
    <servlet-class>controller.CentralController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>config</servlet-name>
    <url-pattern>/config</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logindetail</servlet-name>
    <servlet-class>controller.CentralController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logindetail</servlet-name>
    <url-pattern>/logindetail</url-pattern>
  </servlet-mapping>
</web-app>